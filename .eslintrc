{
  "extends": ["eslint:recommended", "google"],
  "env": {
    "browser": true,
    "node": false
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "new-cap": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-console": "off",
    "max-len": "off",
    "require-jsdoc": "off",
    "brace-style": "off"
  }
}
